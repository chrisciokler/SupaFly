    "turbo": "latest"
  },
  "dependencies": {
    "@inquirer/prompts": "^1.0.0"
  },
  "packageManager": "bun@1.0.18",
  "workspaces": [
    "apps/*",
    "packages/*",
    "cli/*"
  ]
}
